{
  "name": "backend",
  "version": "1.0.0",
  "description": "API REST de produtos",
  "main": "./build/server",
  "scripts": {
    "build": "npx tsc",
    "start": "node .",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts",
    "db": "npx sequelize-cli db:create",
    "db:reset": "npx tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "heroku:api:log": "heroku log --tail --remote heroku",
    "heroku:app:log": "heroku log --tail --remote heroku"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "eslint": "^8.23.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "pg": "^8.8.0",
    "sequelize": "^6.21.4"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/cors": "2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.7",
    "@types/sequelize": "4.28.10",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
